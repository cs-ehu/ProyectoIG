<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>queryBean</managed-bean-name>
  <managed-bean-class>bean.QueryBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mainBean</managed-bean-name>
  <managed-bean-class>bean.MainBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>setBean</managed-bean-name>
  <managed-bean-class>bean.SetBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>Main.xhtml</display-name>
  <from-view-id>/Main.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>query</from-outcome>
   <to-view-id>/QueryAvaiability.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Main.xhtml</display-name>
  <from-view-id>/Main.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>set</from-outcome>
   <to-view-id>/SetAvailability.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>QueryAvaiability.xhtml</display-name>
  <from-view-id>/QueryAvaiability.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>main</from-outcome>
   <to-view-id>/Main.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>Ejercicio2</from-outcome>
   <to-view-id>/Ejercicio2.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>SetAvailability.xhtml</display-name>
  <from-view-id>/SetAvailability.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>main</from-outcome>
   <to-view-id>/Main.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>QueryAvaiability.xhtml</display-name>
  <from-view-id>/QueryAvaiability.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>book</from-outcome>
   <to-view-id>/Book.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Book.xhtml</display-name>
  <from-view-id>/Book.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>query</from-outcome>
   <to-view-id>/QueryAvaiability.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Ejercicio2</display-name>
  <from-view-id>/Ejercicio2.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>query</from-outcome>
   <to-view-id>/QueryAvaiability.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
